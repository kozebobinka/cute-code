{
    "name": "kozebobinka/cute-code",
    "description": "Calculating commissions for already made transactions",
    "type": "project",
    "require": {
        "php": "^8.1",

        "vlucas/phpdotenv": "^5.5",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "CuteCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CuteCode\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Olena Sinelnyk",
            "email": "kozebobinka@gmail.com"
        }
